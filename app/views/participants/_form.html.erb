<%= form_with model: participant do |form| %>
  <%= form.check_box :brings_basegame_english %>
  <%= form.label :brings_basegame_english %>
  <%= form.check_box :brings_basegame_german %>
  <%= form.label :brings_basegame_german %>
  <%= form.check_box :brings_prelude_english %>
  <%= form.label :brings_prelude_english %>
  <%= form.check_box :brings_prelude_german %>
  <%= form.label :brings_prelude_german %>
  <%= form.check_box :brings_hellas_and_elysium %>
  <%= form.label :brings_hellas_and_elysium %>
  <%= form.submit t(".submit") %>
<% end %>
